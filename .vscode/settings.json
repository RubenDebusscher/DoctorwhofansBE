{
  "git.ignoreLimitWarning": true,
  "search.useIgnoreFiles": false,
  "todo-tree.tree.showBadges": false,
  "cSpell.enableFiletypes": [
    "gitignore",
    "http",
    "ini",
    "log",
    "sql"
  ],
  "cSpell.ignoreWords": [
    "data",
    "only",
    "read",
    "set",
    "set read data only"
  ],
  "cSpell.words": [
    "Actorlist",
    "airdate",
    "CLASS",
    "Deathdate",
    "decs",
    "Episodelist",
    "episodestrings",
    "FEFF",
    "framework",
    "gesturestart",
    "Hahmlet",
    "Langcodes",
    "memoriams",
    "pagetype",
    "Parentelem",
    "parsererror",
    "PHPSESSID",
    "prefilter",
    "Prefilter",
    "prefilters",
    "Quoteselection",
    "rnamespace",
    "savepath",
    "Serial",
    "tabindex",
    "tempdir",
    "tmpnam",
    "transform",
    "transient",
    "varlist",
    "Videoselection",
    "Xataface"
  ],
  "discord.enabled": true,
  "phpcs.errorSeverity": 10,
  "files.exclude": {},
  "cSpell.customDictionaries": {
    "custom-dictionary-folder-doctorwhofansbe": {
      "name": "custom-dictionary-folder-doctorwhofansbe",
      "path": "${workspaceFolder:DoctorwhofansBE}/.cspell/custom-dictionary-folder-doctorwhofansbe.txt",
      "addWords": true,
      "scope": "folder"
    }
  }
}