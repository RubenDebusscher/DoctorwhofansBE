{
    "name": "dmzx/scheduledmaintenance",
    "type": "phpbb-extension",
    "description": "Scheduled Maintenance message.",
    "homepage": "https://www.dmzx-web.net",
    "version": "1.0.0",
    "time": "2020-05-13",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "dmzx",
            "email": "info@dmzx-web.net",
            "homepage": "https://www.dmzx-web.net",
            "role": "Extension developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.3.0,<4.4.0@dev"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Scheduled Maintenance",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<4.4.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/scheduled_maintenance",
            "filename": "version_check",
            "ssl": true
        }
    }
}