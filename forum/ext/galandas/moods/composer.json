{
    "name": "galandas/moods",
    "type": "phpbb-extension",
    "description": "User moods extension for phpBB.",
    "homepage": "http://phpbb3world.altervista.org",
    "version": "1.0.1",
    "time": "2024-07-05",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Galandas",
            "email": "manzarek@live.it",
            "homepage": "http://phpbb3world.altervista.org",
            "role": "Amateur Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpbb/phpbb": ">=3.3.0",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Moods",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.12"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/moods",
            "filename": "version_check",
            "ssl": true
        }
    }
}