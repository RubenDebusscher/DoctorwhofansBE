{
    "name": "galandas/moods",
    "type": "phpbb-extension",
    "description": "This Extension Adds the ability for users to set their current mood which is then displayed in profile.",
    "homepage": "http://phpbb3world.altervista.org",
    "version": "1.0.0",
    "time": "2017-06-15",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Galandas",
            "email": "manzarek@live.it",
            "homepage": "http://phpbb3world.altervista.org",
            "role": "Amateur Developer"
        }
    ],
    "require": {
        "php": ">=5.3,<5.4",
        "phpbb/phpbb": ">=3.1.10,<3.2.0"
    },
    "extra": {
        "display-name": "Moods",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.10,<3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/moods",
            "filename": "version_check"
        }
    }
}