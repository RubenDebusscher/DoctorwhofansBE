{
    "name": "galandas/news",
    "type": "phpbb-extension",
    "description": "With this simple extension you can insert news, simple messages, alerts, and more on our forums, all through ACP.",
    "homepage": "http://phpbb3world.altervista.org",
    "version": "1.0.3",
    "time": "2017-03-05",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Galandas",
            "email": "manzarek@live.it",
            "homepage": "http://phpbb3world.altervista.org",
            "role": "Amateur Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "phpbb/phpbb": ">=3.2.0"
    },
    "extra": {
        "display-name": "Simple News Admin",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/simple_news_admin",
            "filename": "version_check"
        }
    }
}