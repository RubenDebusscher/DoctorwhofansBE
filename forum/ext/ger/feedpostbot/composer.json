{
    "name": "ger/feedpostbot",
    "type": "phpbb-extension",
    "description": "Simple feed reader that posts new items in your forum. Supports RSS, RDF and ATOM feeds",
    "homepage": "",
    "version": "1.0.11",
    "time": "2018-05-02",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Ger",
            "email": "",
            "homepage": "https://github.com/GerB",
            "role": ""
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-simplexml": "*",
        "phpbb/phpbb": ">=3.2.2",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Feed post bot",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.2"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/feedpostbot",
            "filename": "version_check"
        }
    }
}