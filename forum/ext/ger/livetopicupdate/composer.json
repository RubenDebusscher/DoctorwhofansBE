{
    "name": "ger/livetopicupdate",
    "type": "phpbb-extension",
    "description": "Shows and loads new posts in viewtopic",
    "homepage": "",
    "version": "1.0.2",
    "time": "2018-01-02",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Ger",
            "email": "",
            "homepage": "https://github.com/GerB",
            "role": ""
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.2,<3.3.0@dev"
