{
    "name": "hifikabin/translate",
    "type": "phpbb-extension",
    "description": "Adds Google Translator to the Forum Header.",
    "homepage": "",
    "version": "1.0.2",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "James Newcombe",
            "username": "hifikabin",
            "email": "webmaster@hifikabin.me.uk",
            "homepage": "http://phpbb.hifikabin.me.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.0"
    },
    "extra": {
        "display-name": "Google Translator",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/google_translator",
            "filename": "version_check"
        }
    }
}