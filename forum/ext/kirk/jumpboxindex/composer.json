{
    "name": "kirk/jumpboxindex",
    "type": "phpbb-extension",
    "description": "Displays the jumpbox on the Board index or additional on User Control Panel.",
    "homepage": "https://www.phpbb.com/community/viewtopic.php?t=2623131",
    "version": "1.1.0",
    "time": "2024-06-21",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Kirk",
            "email": "canam800er@googlemail.com",
            "homepage": "https://reyno41.bplaced.net/phpbb",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3,<8.4.0@dev",
        "phpbb/phpbb": "3.3.0,<3.4.0@dev",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Jumpbox on Index",
        "soft-require": {
            "phpbb/phpbb": "3.3.0,<3.4.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/jumpbox_on_index",
            "filename": "version_check",
            "ssl": true
        }
    }
}