{
    "name": "pwg/pagescrolling",
    "type": "phpbb-extension",
    "description": "Extension adds the scroll arrows to the forum pages & in board's ACP. Arrows will not be visible to guests and search bots.",
    "homepage": "https://github.com/PWG-Extension/pagescrolling",
    "keywords": [
        "PWG",
        "phpBB3",
        "page",
        "scrolling",
        "arrows",
        "up",
        "down"
    ],
    "version": "1.4.0",
    "time": "2018-08-14",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Alexander Kadnikov",
            "username": "Predator",
            "homepage": "http://www.pwg-extension.ru/viewtopic.php?f=2&t=1",
            "email": "None",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3",
        "phpbb/phpbb": ">=3.1.5 || 3.2.*",
        "composer/installers": "~1.0.0"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Page Scrolling",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.5 || 3.2.*"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/page_scrolling",
            "filename": "version_check"
        }
    }
}