{
    "name": "shredder/sitemap",
    "type": "phpbb-extension",
    "description": "phpBB SEO Sitemap for phpBB 3.1-3.3",
    "homepage": "https://www.phpbb-work.ru/",
    "keywords": [
        "phpbb-work",
        "extension",
        "seo",
        "Sitemap"
    ],
    "version": "1.1.3",
    "time": "2015-01-10",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Shredder",
            "homepage": "https://www.phpbb-work.ru/",
            "role": "Developer"
        },
        {
            "name": "ostridm",
            "homepage": "http://strkv.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.0",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "phpBB3 SEO Sitemap",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/seo_sitemap",
            "filename": "version_check",
            "ssl": true
        }
    }
}