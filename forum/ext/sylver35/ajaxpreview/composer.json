{
    "name": "sylver35/ajaxpreview",
    "type": "phpbb-extension",
    "description": "This extension allows you to preview messages, private messages and edit signatures in real time (ajax) directly in the editorial page",
    "homepage": "https://breizhcode.com",
    "version": "1.1.0",
    "time": "2020-01-13",
    "keywords": [
        "phpbb",
        "extension",
        "preview",
        "ajax",
        "message",
        "private message",
        "signature"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "sylver35",
            "homepage": "https://breizhcode.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "phpbb/phpbb": ">=3.2.8",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Breizh Ajax Preview",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.8"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/ajaxpreview",
            "filename": "version_check"
        }
    }
}