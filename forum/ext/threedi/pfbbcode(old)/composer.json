{
    "name": "threedi/pfbbcode",
    "type": "phpbb-extension",
    "description": "Enables BBCodes to Text and String type Profile Fields.",
    "version": "1.0.2",
    "time": "2019-10-07",
    "keywords": [
        "phpbb",
        "extension",
        "profile",
        "field",
        "text",
        "string",
        "BBCode"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "3Di",
            "email": "three3di@hotmail.it",
            "homepage": "https://www.phpbbstudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.2.5,<3.3.0@dev"
    },
    "extra": {
        "display-name": "3Di - BBCode Enabled Profile Fields",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.5,<3.3.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/bbcode_enabled_profile_fields",
            "filename": "version_check"
        }
    }
}