{
    "name": "zukero/userbbcode",
    "type": "phpbb-extension",
    "description": "Adds a [user] bbcode that styles a username and links to his profile",
    "homepage": "github.com/Zukero/userbbcode",
    "version": "1.0.4",
    "time": "2018-04-05",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Zukero",
            "email": "kpochat@gmail.com",
            "homepage": "github.com/Zukero",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.2.0"
    },
    "extra": {
        "display-name": "Username BBCode",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/username_bbcode",
            "filename": "version_check"
        }
    }
}